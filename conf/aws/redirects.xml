<RoutingRules>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>projects/quickies/</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <ReplaceKeyPrefixWith>projects/</ReplaceKeyPrefixWith>
        </Redirect>
    </RoutingRule>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>projects/fun/</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <ReplaceKeyPrefixWith>projects/</ReplaceKeyPrefixWith>
        </Redirect>
    </RoutingRule>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>sites/marktrapp.com/files/</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <ReplaceKeyPrefixWith>files/</ReplaceKeyPrefixWith>
        </Redirect>
    </RoutingRule>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>tags/</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <ReplaceKeyWith>blog/</ReplaceKeyWith>
        </Redirect>
    </RoutingRule>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>taxonomy/term/</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <ReplaceKeyWith>blog/</ReplaceKeyWith>
        </Redirect>
    </RoutingRule>
    <RoutingRule>
        <Condition>
            <KeyPrefixEquals>author/</KeyPrefixEquals>
        </Condition>
        <Redirect>
            <ReplaceKeyWith>about/</ReplaceKeyWith>
        </Redirect>
    </RoutingRule>

    <!-- Posts removed from site -->
